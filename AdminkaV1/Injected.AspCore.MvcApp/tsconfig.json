{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "jsx": "react",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "removeComments": false,

    "outDir": "./wwwroot/dist/",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "skipDefaultLibCheck": true,
    "lib": [ "es6", "dom" ]
    //,
    //"types": [ "webpack-env" ]
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
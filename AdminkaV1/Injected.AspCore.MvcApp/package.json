    "@types/bootstrap": "4.1.0",
    "@types/jquery": "3.3.1",
    "@types/node": "10.0.8",
    "autoprefixer": "8.4.1",
    "awesome-typescript-loader": "5.0.0",
    "babel-loader": "8.0.0-beta.2",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "expose-loader": "0.7.5",
    "file-loader": "1.1.11",
    "gulp": "3.9.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
